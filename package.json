{
  "name": "sleepermdleague",
  "version": "0.1.0",
  "homepage": "https://victormrecabarren.github.io/mdleaguedata/",
  "private": true,
  "dependencies": {
    "@testing-library/user-event": "^13.5.0",
    "ag-grid-react": "^30.2.1",
    "antd": "^5.11.3",
    "bootstrap": "^5.3.2",
    "chart.js": "^4.4.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^7.6.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "postbuild": "cp build/index.html build/404.html",
    "predeploy": "npm run build",
    "prepare": "husky",
    "deploy": "gh-pages -d build",
    "postinstall": "husky install",
    "prettier:write": "prettier --write .",
    "prettier:staged": "sh -c 'git diff --cached --name-only --diff-filter=ACM | xargs prettier --check || (echo \"\\n\\033[31mPrettier check failed.\\033[31m Run \\`npm run prettier:staged:write\\` to fix formatting.\" && exit 1)'",
    "prettier:staged:write": "sh -c 'git diff --cached --name-only --diff-filter=ACM | xargs prettier --write'",
    "lint": "eslint .",
    "lint:staged": "sh -c \"git diff --cached --name-only --diff-filter=ACM | grep -E '\\\\.(js|jsx|ts|tsx)$' | xargs eslint\"",
    "lint:commit": "npm run lint:staged && npm run prettier:staged",
    "test:commit": "react-scripts test --watchAll=false"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/css": "^0.10.0",
    "@eslint/js": "^9.33.0",
    "@eslint/json": "^0.13.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@vitejs/plugin-react": "^5.0.0",
    "gh-pages": "^6.1.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "vite": "^7.1.2",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0"
  }
}
