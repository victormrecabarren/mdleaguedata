{
  "name": "sleepermdleague",
  "version": "0.1.0",
  "homepage": "https://victormrecabarren.github.io/mdleaguedata/",
  "private": true,
  "dependencies": {
    "@testing-library/user-event": "^13.5.0",
    "ag-grid-react": "^30.2.1",
    "antd": "^5.11.3",
    "bootstrap": "^5.3.2",
    "chart.js": "^4.4.0",
    "eslint": "^8.57.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^7.6.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:commit": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "prepare": "husky",
    "postinstall": "husky install",
    "prettier:write": "prettier --write .",
    "prettier:staged": "sh -c 'git diff --cached --name-only --diff-filter=ACM | grep -E \"\\\\.(js|jsx|ts|tsx|json|css|md)$\" | xargs -r prettier --check 2>&1 | grep -v \"Run Prettier with --write to fix\" || (echo \"\\n❌ Prettier check failed. Run \\`npm run prettier:staged:write\\` to fix formatting.\" && exit 1)'",
    "prettier:staged:write": "sh -c 'git diff --cached --name-only --diff-filter=ACM | xargs prettier --write'",
    "lint": "eslint .",
    "lint:staged": "sh -c \"git diff --cached --name-only --diff-filter=ACM | grep -E '\\\\.(js|jsx|ts|tsx)$' | xargs eslint\"",
    "lint:commit": "npm run lint:staged && npm run prettier:staged",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "postbuild": "cp build/index.html build/404.html",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@vitejs/plugin-react": "^5.0.0",
    "gh-pages": "^6.1.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "vite": "^7.1.2",
    "@eslint/js": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0"
  }
}
