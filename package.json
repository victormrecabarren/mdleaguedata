{
  "name": "sleepermdleague",
  "version": "0.1.0",
  "homepage": "https://victormrecabarren.github.io/mdleaguedata/",
  "private": true,
  "dependencies": {
    "@testing-library/user-event": "^13.5.0",
    "ag-grid-react": "^30.2.1",
    "antd": "^5.11.3",
    "bootstrap": "^5.3.2",
    "chart.js": "^4.4.0",
    "eslint": "^8.57.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^7.6.3",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:commit": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "prepare": "husky",
    "postinstall": "husky install",
    "prettier:write": "prettier --write .",
    "prettier:staged": "sh -c 'git diff --cached --name-only --diff-filter=ACM | xargs prettier --check'",
    "lint": "eslint .",
    "lint:staged": "sh -c \"git diff --cached --name-only --diff-filter=ACM | grep -E '\\\\.(js|jsx|ts|tsx)$' | xargs eslint\"",
    "lint:commit": "npm run lint:staged && npm run prettier:staged",
    "postbuild": "cp build/index.html build/404.html",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "gh-pages": "^6.1.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2"
  }
}
